<?xml version="1.0" encoding="utf-8"?>
<module>
  <assemblies>
    <!-- This adds the Alloy template assembly to the "default module" -->
    <add assembly="WebClient" />
  </assemblies>
  <clientResources>
    <add name="epi-cms.widgets.base" path="Styles/Styles.css" resourceType="Style"/>
    <add name="epi-cms.widgets.base" path="Scripts/customtool/customtoolview.js" resourceType="Script">
    </add>
  </clientResources>
 
  <dojoModules>
    <add name="custom" path="Scripts" />
  </dojoModules>
  <clientModule initializer="custom.customtool.customtoolview"> 
    <moduleDependencies>
      <add dependency="Shell" />
      <add dependency="CMS" type="Require RunAfter" />
    </moduleDependencies>
    </clientModule>
  
</module>