<?xml version="1.0" encoding="utf-8"?>
<module>
  <assemblies>
    <!-- This adds the Alloy template assembly to the "default module" -->
    <add assembly="WebClient" />
  </assemblies>
  <clientResources>
    <add name="epi-cms.widgets.base" path="Styles/Styles.css" resourceType="Style"/>
    <add name="epi-cms.widgets.base" path="Scripts/customtool/customtoolview.js" resourceType="Script"/>
  </clientResources>
  <dojo>
    <!-- Add a mapping from alloy to ~/ClientResources/Scripts to the dojo loader configuration -->
    <paths>
      <add name="WebClient" path="Scripts" />
    </paths>
  </dojo>
  <dojoModules>
    <add name="custom" path="Scripts" />
    <add name="jondjones" path="Scripts" />
  </dojoModules>
  <clientModule initializer="custom.editors.divider">
    <moduleDependencies>
      <add dependency="Shell" />
      <add dependency="CMS" type="Require RunAfter" />
    </moduleDependencies>
  </clientModule>
</module>